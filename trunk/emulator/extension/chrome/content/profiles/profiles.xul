              <vbox id="jwe-profiles-device-panel-datanetwork" class="jwe-profiles-inner-panel" style="display: none;">
                <groupbox flex="1" style="width: 90%;">
                  <caption label="&jwe.profiles.devices.datanetwork.enabled;" class="jwe-defaultText" />
                  <vbox pack="start">                    
                    <checkbox flex="1" label="&jwe.profiles.devices.datanetwork.bluetooth;" oncommand="jwe_Profiles.change('dDNetwork'); jwe_Profiles.adjustNetwork();"  id="jwe-profiles-devices-datanetwork-bluetooth"/>                    
                    <checkbox flex="1" label="&jwe.profiles.devices.datanetwork.edge;" oncommand="jwe_Profiles.change('dDNetwork'); jwe_Profiles.adjustNetwork();"  id="jwe-profiles-devices-datanetwork-edge"/>
                    <checkbox flex="1" label="&jwe.profiles.devices.datanetwork.evdo;" oncommand="jwe_Profiles.change('dDNetwork'); jwe_Profiles.adjustNetwork();"  id="jwe-profiles-devices-datanetwork-evdo"/>                    
                    <checkbox flex="1" label="&jwe.profiles.devices.datanetwork.gprs;" oncommand="jwe_Profiles.change('dDNetwork'); jwe_Profiles.adjustNetwork();"  id="jwe-profiles-devices-datanetwork-gprs"/>
