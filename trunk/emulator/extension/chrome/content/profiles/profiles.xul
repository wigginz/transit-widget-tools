<?xml version="1.0"?>
<?xml-stylesheet href="chrome://global/skin/" type="text/css"?>
<?xml-stylesheet href="chrome://transit-emulator/skin/skin.css" type="text/css"?>
<!DOCTYPE jilemulator SYSTEM "chrome://transit-emulator/locale/translations.dtd">



<window xmlns="http://www.mozilla.org/keymaster/gatekeeper/there.is.only.xul"
        xmlns:html="http://www.w3.org/1999/xhtml"
        id="jwe-profiles-panel"
        title="&jwe.profiles.title;"
        onload="jwe_Profiles.init();"
        onclose="return(closeForm('&jwe.profiles.ok.confirm;'));">

  <script src="chrome://transit-emulator/content/utils/utils.js" />
  <script src="chrome://transit-emulator/content/profiles/profiles.js" />

  <vbox flex="1">
    
    <tabbox id="jwe-profiles-content">
    
      <tabs>
         <tab label="&jwe.profiles.tab.devices;" oncommand="jwe_Profiles.loadDeviceProfiles();"/>
         <tab label="&jwe.profiles.tab.messaging;" oncommand="jwe_Profiles.loadMessageProfiles();"/>
         <tab label="&jwe.profiles.tab.pim;" oncommand="jwe_Profiles.loadPIMProfiles();"/>
         <tab label="&jwe.profiles.tab.defaults;" oncommand="jwe_loadDefaults();"/>
      </tabs>
      
      <tabpanels align="center">
      
        <tabpanel flex="1"> 
        
          <hbox>
            <vbox align="center" id="jwe-profiles-sel-device-panel">
              <listbox id="jwe-profiles-sel-device-list" flex="1" style="width: 170px;" seltype="single" onselect="jwe_Profiles.changeSelectedDeviceProfile();"/>
              <hbox pack="center" class="jwe-profiles-buttons">
                <button label="&jwe.profiles.device.add;" id="jwe-profiles-device-add" oncommand="jwe_newDeviceProfile();"/> 
                <button label="&jwe.profiles.device.remove;" id="jwe-profiles-device-remove" oncommand="jwe_deleteDeviceProfile('&jwe.profiles.device.remove.confirm;');"  disabled="true"/>
              </hbox>
            </vbox>
            
            <vbox class="jwe-profiles-panel-container">

              <hbox align="center" pack="end">
                <label id="jwe-profiles-sel-device-api-caption" style="font-weight: bold;"/>
                <menulist label="&jwe.profiles.devices.tab.settings;" align="right" id="jwe-profiles-sel-device-api-list">
                  <menupopup>
                    <menuitem label="&jwe.profiles.devices.tab.settings;" selected="true" oncommand="jwe_changeDevicePanel('jwe-profiles-device-panel-general');" id="jwe-subtab-dGeneral"/>
                    <menuitem label="&jwe.profiles.devices.tab.widget;" oncommand="jwe_changeDevicePanel('jwe-profiles-device-panel-widget'); jwe_Profiles.loadSubTab('dWidget');" id="jwe-subtab-dWidget"/>
                    <menuitem label="&jwe.profiles.devices.tab.device;" oncommand="jwe_changeDevicePanel('jwe-profiles-device-panel-device'); jwe_Profiles.loadSubTab('dDevice');" id="jwe-subtab-dDevice"/>
                    <menuitem label="&jwe.profiles.devices.tab.account;" oncommand="jwe_changeDevicePanel('jwe-profiles-device-panel-account'); jwe_Profiles.loadSubTab('dAccount');" id="jwe-subtab-dAccount"/>
                    <menuitem label="&jwe.profiles.devices.tab.deviceinfo;" oncommand="jwe_changeDevicePanel('jwe-profiles-device-panel-deviceinfo'); jwe_Profiles.loadSubTab('dDInfo');" id="jwe-subtab-dDInfo"/>
                    <menuitem label="&jwe.profiles.devices.tab.datanetwork;" oncommand="jwe_changeDevicePanel('jwe-profiles-device-panel-datanetwork'); jwe_Profiles.loadSubTab('dDNetwork');" id="jwe-subtab-dDNetwork"/>
                    <menuitem label="&jwe.profiles.devices.tab.devicestate;" oncommand="jwe_changeDevicePanel('jwe-profiles-device-panel-devicestate'); jwe_Profiles.loadSubTab('dDState');" id="jwe-subtab-dDState"/>
                    <menuitem label="&jwe.profiles.devices.tab.accelerometer;" oncommand="jwe_changeDevicePanel('jwe-profiles-device-panel-accelerometer'); jwe_Profiles.loadSubTab('dAccel');" id="jwe-subtab-dAccel"/>
                    <menuitem label="&jwe.profiles.devices.tab.config;" oncommand="jwe_changeDevicePanel('jwe-profiles-device-panel-config'); jwe_Profiles.loadSubTab('dConfig');" id="jwe-subtab-dConfig"/>
                    <menuitem label="&jwe.profiles.devices.tab.position;" oncommand="jwe_changeDevicePanel('jwe-profiles-device-panel-position'); jwe_Profiles.loadSubTab('dPosition');" id="jwe-subtab-dPosition"/>
                    <menuitem label="&jwe.profiles.devices.tab.power;" oncommand="jwe_changeDevicePanel('jwe-profiles-device-panel-power'); jwe_Profiles.loadSubTab('dPower');" id="jwe-subtab-dPower"/>
                    <menuitem label="&jwe.profiles.devices.tab.radio;" oncommand="jwe_changeDevicePanel('jwe-profiles-device-panel-radio'); jwe_Profiles.loadSubTab('dRadio');" id="jwe-subtab-dRadio"/>
                    <menuitem label="&jwe.profiles.devices.tab.multimedia;" oncommand="jwe_changeDevicePanel('jwe-profiles-device-panel-multimedia'); jwe_Profiles.loadSubTab('dMultimedia');" id="jwe-subtab-dMultimedia"/>
                    <menuitem label="&jwe.profiles.devices.tab.telephony;" oncommand="jwe_changeDevicePanel('jwe-profiles-device-panel-telephony'); jwe_Profiles.loadSubTab('dTelephony');" id="jwe-subtab-dTelephony"/>
                  </menupopup>
                </menulist>
              </hbox>

              <vbox id="jwe-profiles-device-panel-general" class="jwe-profiles-inner-panel">
                <label flex="1" id="jwe-profiles-device-panel-general-uuid" style="margin-bottom: 20px;"/>
                <groupbox>
                  <caption label="&jwe.profiles.devices.settings.assoc;" class="jwe-defaultText" />
                  <vbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.settings.messaging;" class="jwe-defaultText" style="width: 10em;"/>
                      <menulist  align="right" oncommand="jwe_Profiles.change('dGeneral');" id="jwe-profiles-device-panel-general-msglist"/>
                    </hbox>
                    
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.settings.pim;" class="jwe-defaultText" style="width: 10em;"/>
                      <menulist align="right" oncommand="jwe_Profiles.change('dGeneral');" id="jwe-profiles-device-panel-general-pimlist"/>
                    </hbox>
                  </vbox>
                </groupbox>
                <groupbox>
                  <caption label="&jwe.profiles.devices.settings.specs;" class="jwe-defaultText" />
                  <vbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.settings.specs.api;" class="jwe-defaultText" style="width: 10em;"/>
                      <menulist align="right" oncommand="jwe_Profiles.change('dGeneral');" id="jwe-profiles-device-panel-general-apispec">
                        <menupopup>
                          <menuitem label="1.1r4" value="1.1r4"/>
                          <menuitem label="1.2.2" value="1.2.2"/>
                        </menupopup>
                      </menulist>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.settings.specs.extensions;" class="jwe-defaultText" style="width: 10em;"/>
                      <vbox>
                        <checkbox flex="1" label="&jwe.profiles.devices.settings.specs.extensions.samsung360;" oncommand="jwe_Profiles.change('dGeneral');"  id="jwe-profiles-device-panel-general-extensions-samsung360"/>
                      </vbox>
                    </hbox>
                  </vbox>
                </groupbox>
              </vbox>

              <vbox id="jwe-profiles-device-panel-widget" class="jwe-profiles-inner-panel" style="display: none;">
                <groupbox>
                  <caption label="&jwe.profiles.widget.prefs;" class="jwe-defaultText" />
                  <vbox>
                    <listbox style="width: 500px;" id="jwe-profiles-device-panel-widgets" onselect="jwe_selectEmulatedWidget();" ondblclick="jwe_modifyEmulatedWidget();">
                      <listhead>
                        <listheader label="&jwe.profiles.widget.prefs.name;" />
                        <listheader label="&jwe.profiles.widget.prefs.version;" />
                        <listheader label="&jwe.profiles.widget.prefs.author;" />
                      </listhead>
                      <listcols>
                        <listcol style="width:  275px;"/>
                        <listcol style="width:  100px;"/>
                        <listcol style="width:  155px;"/>
                      </listcols>
                    </listbox>
                    <hbox pack="end" class="jwe-profiles-buttons">
                      <button label="&jwe.profiles.widget.prefs.new;" id="jwe-profiles-widgets-prefs-new" oncommand="jwe_newEmulatedWidget();" />
                      <button label="&jwe.profiles.widget.prefs.modify;" id="jwe-profiles-widgets-prefs-modify" oncommand="jwe_modifyEmulatedWidget();" />
                      <button label="&jwe.profiles.widget.prefs.preferences;" id="jwe-profiles-widgets-prefs-preferences" oncommand="jwe_openWidgetPrefs();" />
                      <button label="&jwe.profiles.widget.prefs.delete;" id="jwe-profiles-widgets-prefs-delete" oncommand="jwe_deleteEmulatedWidget();" /> 
                    </hbox>
                    <description>&jwe.profiles.widget.prefs.warning;</description>                  
                  </vbox>
                </groupbox>
              </vbox>

              <vbox id="jwe-profiles-device-panel-device" class="jwe-profiles-inner-panel" style="display: none;" flex="1">
                <groupbox flex="1">
                  <caption label="&jwe.profiles.devices.device.values;" class="jwe-defaultText" />
                  <vbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.device.clip;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-device-clip" multiline="true" rows="3" cols="35" onchange="jwe_Profiles.change('dDevice');"/>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.device.engine.name;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-device-engine-name" size="25" onchange="jwe_Profiles.change('dDevice');"/>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.device.engine.prov;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-device-engine-prov" size="25" onchange="jwe_Profiles.change('dDevice');"/>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.device.engine.ver;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-device-engine-ver" size="10" onchange="jwe_Profiles.change('dDevice');"/>
                    </hbox>
                  </vbox>
                </groupbox>
                
                <groupbox flex="1">
                  <caption label="&jwe.profiles.devices.filesystems;" class="jwe-defaultText" />
                  <vbox>                        
                    <listbox rows="3" style="width: 400px;" id="jwe-profiles-devices-device-filesystems" onselect="jwe_selectFileSystem();" ondblclick="jwe_modifyFileSystem();">
                      <listhead>
                        <listheader label="&jwe.profiles.devices.filesystems.path;" />                      
                        <listheader label="&jwe.profiles.devices.filesystems.localpath;" />
                        <listheader label="&jwe.profiles.devices.filesystems.size;" />
                      </listhead>
                      <listcols>
                        <listcol style="width:  150px;"/>
                        <listcol style="width:  215px;"/>
                        <listcol style="width:  150px;"/>
                      </listcols>
                    </listbox>                          
                    <hbox pack="end" class="jwe-profiles-buttons">
                      <button label="&jwe.profiles.devices.filesystems.new;" id="jwe-profiles-devices-filesystems-new" oncommand="jwe_newFileSystem();" />
                      <button label="&jwe.profiles.devices.filesystems.modify;" id="jwe-profiles-devices-filesystems-modify" oncommand="jwe_modifyFileSystem();" />
                      <button label="&jwe.profiles.devices.filesystems.delete;" id="jwe-profiles-devices-filesystems-delete" oncommand="jwe_deleteFileSystem();" /> 
                    </hbox>
                  </vbox>
                </groupbox>
                
                <groupbox flex="1">
                  <caption label="&jwe.profiles.devices.apps;" class="jwe-defaultText" />
                  <vbox flex="1">
                    <checkbox flex="1" label="&jwe.profiles.devices.apps.alarm;" value="ALARM" id="jwe-profiles-devices-device-app-alarm" oncommand="jwe_Profiles.change('dDevice');"/>
                    <checkbox flex="1" label="&jwe.profiles.devices.apps.browser;" value="BROWSER" id="jwe-profiles-devices-device-app-browser" oncommand="jwe_Profiles.change('dDevice');"/>
                    <checkbox flex="1" label="&jwe.profiles.devices.apps.calculator;" value="CALCULATOR" id="jwe-profiles-devices-device-app-calculator" oncommand="jwe_Profiles.change('dDevice');"/>
                    <checkbox flex="1" label="&jwe.profiles.devices.apps.calendar;" value="CALENDAR" id="jwe-profiles-devices-device-app-calendar" oncommand="jwe_Profiles.change('dDevice');"/>
                    <checkbox flex="1" label="&jwe.profiles.devices.apps.camera;" value="CAMERA" id="jwe-profiles-devices-device-app-camera" oncommand="jwe_Profiles.change('dDevice');"/>
                    <checkbox flex="1" label="&jwe.profiles.devices.apps.contacts;" value="CONTACTS" id="jwe-profiles-devices-device-app-contacts" oncommand="jwe_Profiles.change('dDevice');"/>
                    <checkbox flex="1" label="&jwe.profiles.devices.apps.files;" value="FILES" id="jwe-profiles-devices-device-app-files" oncommand="jwe_Profiles.change('dDevice');"/>
                    <checkbox flex="1" label="&jwe.profiles.devices.apps.games;" value="GAMES" id="jwe-profiles-devices-device-app-games" oncommand="jwe_Profiles.change('dDevice');"/>
                    <checkbox flex="1" label="&jwe.profiles.devices.apps.mail;" value="MAIL" id="jwe-profiles-devices-device-app-mail" oncommand="jwe_Profiles.change('dDevice');"/>
                    <checkbox flex="1" label="&jwe.profiles.devices.apps.mediaplayer;" value="MEDIAPLAYER" id="jwe-profiles-devices-device-app-mediaplayer" oncommand="jwe_Profiles.change('dDevice');"/>
                    <checkbox flex="1" label="&jwe.profiles.devices.apps.messaging;" value="MESSAGING" id="jwe-profiles-devices-device-app-messaging" oncommand="jwe_Profiles.change('dDevice');"/>
                    <checkbox flex="1" label="&jwe.profiles.devices.apps.phonecall;" value="PHONECALL" id="jwe-profiles-devices-device-app-phonecall" oncommand="jwe_Profiles.change('dDevice');"/>
                    <checkbox flex="1" label="&jwe.profiles.devices.apps.pictures;" value="PICTURES" id="jwe-profiles-devices-device-app-pictures" oncommand="jwe_Profiles.change('dDevice');"/>
                    <checkbox flex="1" label="&jwe.profiles.devices.apps.progmgr;" value="PROG_MANAGER" id="jwe-profiles-devices-device-app-prog_manager" oncommand="jwe_Profiles.change('dDevice');"/>
                    <checkbox flex="1" label="&jwe.profiles.devices.apps.settings;" value="SETTINGS" id="jwe-profiles-devices-device-app-settings" oncommand="jwe_Profiles.change('dDevice');"/>
                    <checkbox flex="1" label="&jwe.profiles.devices.apps.tasks;" value="TASKS" id="jwe-profiles-devices-device-app-tasks" oncommand="jwe_Profiles.change('dDevice');"/>
                    <checkbox flex="1" label="&jwe.profiles.devices.apps.widgetmgr;" value="WIDGET_MANAGER" id="jwe-profiles-devices-device-app-widget_manager" oncommand="jwe_Profiles.change('dDevice');"/>
                  </vbox>
                </groupbox>
                
              </vbox>
                    
              <vbox id="jwe-profiles-device-panel-account" class="jwe-profiles-inner-panel" style="display: none;">
                <groupbox flex="1">
                  <caption label="&jwe.profiles.devices.account;" class="jwe-defaultText" />
                  <vbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.account.phonemsisdn;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-account-phonemsisdn" size="25" onchange="jwe_Profiles.change('dAccount');"/>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.account.opname;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-account-opname" size="25" onchange="jwe_Profiles.change('dAccount');"/>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.account.userid;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-account-userid" size="25" onchange="jwe_Profiles.change('dAccount');"/>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.account.balance;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-account-balance" size="10" onchange="jwe_Profiles.change('dAccount');"/>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.account.type;" class="jwe-defaultText" style="width: 14em;"/>
                      <menulist id="jwe-profiles-devices-account-type" align="right" oncommand="jwe_Profiles.change('dAccount');">
                        <menupopup>
                          <menuitem label="&jwe.profiles.devices.account.type.prepaid;" value="prepaid"/>
                          <menuitem label="&jwe.profiles.devices.account.type.postpaid;" value="postpaid"/>
                          <menuitem label="&jwe.profiles.devices.account.type.other;" value="other"/>
                        </menupopup>
                      </menulist>
                    </hbox>
                  </vbox>
                </groupbox>
              </vbox>
              
              <vbox id="jwe-profiles-device-panel-deviceinfo" class="jwe-profiles-inner-panel" style="display: none;">
                <groupbox flex="1">
                  <caption label="&jwe.profiles.devices.ownerinfo;" class="jwe-defaultText" />
                  <vbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.ownerinfo.address;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-ownerinfo-address" size="25" onchange="jwe_Profiles.change('dDInfo');"/>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.ownerinfo.company;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-ownerinfo-company" size="25" onchange="jwe_Profiles.change('dDInfo');"/>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.ownerinfo.email;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-ownerinfo-email" size="25" onchange="jwe_Profiles.change('dDInfo');"/>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.ownerinfo.fullname;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-ownerinfo-fullname" size="25" onchange="jwe_Profiles.change('dDInfo');"/>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.ownerinfo.title;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-ownerinfo-title" size="25" onchange="jwe_Profiles.change('dDInfo');"/>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.ownerinfo.homephone;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-ownerinfo-homephone" size="25" onchange="jwe_Profiles.change('dDInfo');"/>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.ownerinfo.mobilephone;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-ownerinfo-mobilephone" size="25" onchange="jwe_Profiles.change('dDInfo');"/>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.ownerinfo.workphone;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-ownerinfo-workphone" size="25" onchange="jwe_Profiles.change('dDInfo');"/>
                    </hbox>
                  </vbox>
                </groupbox>
                
                <groupbox flex="1">
                  <caption label="&jwe.profiles.devices.deviceinfo;" class="jwe-defaultText" />
                  <vbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.deviceinfo.colordepth;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-deviceinfo-colordepth" size="5" onchange="jwe_Profiles.change('dDInfo');" min="0" type="number"/>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.deviceinfo.firmware;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-deviceinfo-firmware" size="15" onchange="jwe_Profiles.change('dDInfo');"/>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.deviceinfo.manufacturer;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-deviceinfo-manufacturer" size="25" onchange="jwe_Profiles.change('dDInfo');"/>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.deviceinfo.model;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-deviceinfo-model" size="25" onchange="jwe_Profiles.change('dDInfo');"/>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.deviceinfo.os;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-deviceinfo-os" size="25" onchange="jwe_Profiles.change('dDInfo');"/>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.deviceinfo.height;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-deviceinfo-height" size="5" onchange="jwe_Profiles.change('dDInfo');" min="0" type="number"/>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.deviceinfo.width;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-deviceinfo-width" size="5" onchange="jwe_Profiles.change('dDInfo');" min="0" type="number"/>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.deviceinfo.software;" class="jwe-defaultText" style="width: 14em;"/>
                      <textbox id="jwe-profiles-devices-deviceinfo-software" size="25" onchange="jwe_Profiles.change('dDInfo');"/>
                    </hbox>
                    <hbox align="center">
                      <label value="&jwe.profiles.devices.deviceinfo.memory;" class="jwe-defaultText" style="width: 14em;"/>
