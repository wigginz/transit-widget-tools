  <html:script>
  <![CDATA[  
    function resetForm(type, message)
    {
      if ( confirm(message) )
        jwe_Profiles.reset(type);
    }
    
    function saveForm(type, message, successMessage)
    {
      if ( confirm(message) )
        jwe_Profiles.save(type, successMessage);
    }
    
    function closeForm(message)
    {
      if ( jwe_Profiles.unsavedChanges() )
      {
        if ( confirm(message) )
          window.close();
        else
          return(false);
      }
      window.close();
    }
  ]]>
  </html:script>

</window>